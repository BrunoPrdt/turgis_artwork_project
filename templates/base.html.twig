                        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item" href="{{ path('watercolors_show') }}">Aquarelles & Gouache</a>
                            <a class="dropdown-item" href="{{ path('classicalOil_show') }}">&Agrave; huile, style classique</a>
                            <a class="dropdown-item" href="{{ path('fantasticalOil_show') }}">&Agrave; huile, style fantastique</a>
                            <a class="dropdown-item" href="{{ path('surrealistOil_show') }}">&Agrave; huile, style surréaliste, thérapeutique & portrait</a>
                            <a class="dropdown-item" href="{{ path('laqueredOil_swhow') }}">Peintures laquées et vitraux</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown d-none">
                        <a class="nav-link dropdown-toggle text-white" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Oeuvres Numèriques</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item disabled" href="{{ path('numarch_show') }}">Numérique Architecture </a>
                            <a class="dropdown-item disabled" href="{{ path('numerics_show') }}">Numérique</a>
                            <a class="dropdown-item disabled" href="{{ path('nzywuikx_show') }}">Zywuikx</a>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="{{ path('eggs_page') }}">Oeufs décoratifs</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="{{ path('turgis.contact')}}">Contact</a>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    {% if not app.user %}
                    <li>
                        <a class="nav-link text-white" href="{{ path('login') }}"><i class="fas fa-sign-in-alt"></i>&nbsp;&nbsp;Login</a>
                    </li>
                    {% else %}
                    <li class="nav-item">
                        <a class="nav-link text-white" href="{{ path('logout') }}"><i class="fas fa-sign-out-alt"></i>&nbsp;&nbsp;Déconnexion</a>
                    </li>
                    {% endif %}

                </ul>
            </div>
            </nav>
