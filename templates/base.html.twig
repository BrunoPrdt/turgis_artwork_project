        <link rel="stylesheet" href="{{ asset('css/style.css') }}">

       

        {% block stylesheets %}{% endblock %}
        
    </head>
    <body>
           
                 <header id='header'>
                    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
                    <a class="nav-link text-white" href="{{ path('home') }}">Turgis Arts Graphiques</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarColor02">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active dropdown d-none">
                        <a class="nav-link dropdown-toggle text-white " href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        Dessins<span class="sr-only">(current)</span></a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item disabled" href="{{ path('croquis_show')}}">Crayon, Croquis</a>
                            <a class="dropdown-item disabled" href="{{ path('china_show')}}">Encre Chine</a>
                            <a class="dropdown-item disabled" href="{{ path('apod_show')}}"> Encre de chine Apocalypse </a>
