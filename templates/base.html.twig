        {% block body %}{% endblock %}

        <!-- Footer -->
<footer class="page-footer font-small bg-dark ">

  <!-- Copyright -->
  <div class="footer-copyright text-center text-white  py-3">© 2019 Copyright</div>
  <!-- Copyright -->

</footer>
